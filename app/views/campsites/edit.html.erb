<%= form_for @campsite do |f| %>
    <%= f.label :site_number %>
    <%= f.text_field :site_number %>
    <%= f.label :location %>
    <%= f.text_field :location %>
    <%= f.label :accomodation %>
    <%= f.text_field :accomodation %>
    <%= f.label :price %>
    <%= f.text_field :price %>
    <%= f.label :length_of_stay %>
    <%= f.text_field :length_of_stay %>
    <%= f.submit %> 
   
<% end %>